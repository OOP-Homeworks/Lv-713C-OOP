using System.Globalization;
using System.Collections.Generic;
using System;
using System.Linq;

namespace program
{
    public static class Problem
    {
        public static void Main()
        {
            Console.WriteLine(MaxNumber(5,3,1));
        }
        public static bool InRange(float numberOne, float numberTwo, float numberThree) 
        {
            int minRange = -5;
            int maxRange = 5;
            bool one = true;
            bool two = false;

            return numberOne > minRange && numberOne < maxRange;
        }
        public static int MaxNumber(int numberOne, int numberTwo, int numberThree)
        {
            var numbers = new List<int>() { numberOne, numberTwo, numberThree };
            return numbers.Max();
        }

        public static string ErrorsDecode(int errorNum)
        {
            return "";
        }
    }
